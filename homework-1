
// Sock Merchant

	let n = 10
	let arr = [1 ,1, 3, 1, 2, 1, 3, 3, 3, 3 ]


	let arrToSet = Set(ar)
	var 	numOfMatching = 0

	for i in arrToSet  {
    		let dup : Int = arr.filter({$0 == i}).count
    
    		numOfMatching += Int(dup/2)
	}

	print(numOfMatching)


